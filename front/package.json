  },
  "homepage": "https://github.com/betagouv/compta#readme",
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "compression": "^1.7.4",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^13.0.4",
    "next-images": "^0.10.6",
    "pm2": "^3.5.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-select": "^3.0.4"
  },
